using System;

class Program {
    static void Main(string[] args) {
        double raio, area;
        const double pi = 3.14159;

        // Leitura do raio
        raio = double.Parse(Console.ReadLine());

        // Cálculo da área
        area = pi * Math.Pow(raio, 2);

        // Exibição do resultado com 4 casas decimais
        Console.WriteLine($"A={area:F4}");
    }
}
